      "import": "./dist/vite.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.2.25"
  },
  "dependencies": {
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "crypto-js": "^4.1.1",
    "serialize-javascript": "^6.0.1",
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
